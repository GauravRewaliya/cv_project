<style>
    *{
      margin: 0px;
    }
                  body{
                    font-family: Arial, sans-serif;
                    /* font-family: Helvetica, Arial, sans-serif; */
                  }
                  .icon{
                    fill: white;
                    width: 1em;
                  height: 1em;
                  }
                  svg{
                    margin-right: 12px;
                    margin-left: 8px;
                  }
  </style>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css" integrity="sha512-iecdLmaskl7CVkqkXNQ/ZH/XLlvWZOJyj7Yy7tcenmpD1ypASozpmT/E0iPtmFIB46ZmdtAc9eNBvH0H/ZpiBw==" crossorigin="anonymous" referrerpolicy="no-referrer" />
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
  
  <div class="bg-light ">
      <div class="container p-5">
          <div class="row">
              <!-- <div class="col-lg-4 bg-dark text-white text-center py-4"> -->
              <div class="col-5 bg-dark text-white text-center py-4">
                  <div class="header-left">
                      <% if !@curriculum_vitae.image.blank?%>
           <%= image_tag @curriculum_vitae.image.variant(resize_to_limit: [nil, 120]) , class: "img-thumbnail rounded-circle " , style: "height: 130px; width: 150px;" if curriculum_vitae.image.attached? %>
                      <!-- <img src="<%= url_for(@curriculum_vitae.image.variant(resize_to_limit: [nil, 120])) %>"
                      alt="not found" class="img-thumbnail rounded-circle " style="height: 130px; width: 150px;"> -->
                      <%end%>
                  <h1 class="display-5"> <%= curriculum_vitae.candidate&.name %></h1>
                      <h4 class="lead text-uppercase text-white-50 mb-4"><%= curriculum_vitae.objective %></h4>
  
                  </div>
                  <div >
                  <% profile=curriculum_vitae.candidate.profile %>
                  <h5 class="text-uppercase bg-white text-dark py-2 rounded-pill">info</h5>
                      <ul class="list-unstyled text-white-50 ml-3 py-2 text-left">
                        <li class="list-item">
                            <!-- <i class="fas fa-mobile-alt mx-4"></i><%= profile.contact %> -->
                            <svg class="icon" xmlns="http://www.w3.org/2000/svg" height="1em" viewBox="0 0 320 512"><!--! Font Awesome Free 6.4.0 by @fontawesome - https://fontawesome.com License - https://fontawesome.com/license (Commercial License) Copyright 2023 Fonticons, Inc. --><path d="M272 0H48C21.5 0 0 21.5 0 48v416c0 26.5 21.5 48 48 48h224c26.5 0 48-21.5 48-48V48c0-26.5-21.5-48-48-48zM160 480c-17.7 0-32-14.3-32-32s14.3-32 32-32 32 14.3 32 32-14.3 32-32 32zm112-108c0 6.6-5.4 12-12 12H60c-6.6 0-12-5.4-12-12V60c0-6.6 5.4-12 12-12h200c6.6 0 12 5.4 12 12v312z"/></svg>
                            <%= profile.contact %> 
                        </li>
                        <li class="list-item">
                            <!-- <i class="fas fa-envelope-open-text mx-4"></i> <%= profile.email %> -->
                            <svg class="icon" xmlns="http://www.w3.org/2000/svg" height="1em" viewBox="0 0 512 512"><!--! Font Awesome Free 6.4.0 by @fontawesome - https://fontawesome.com License - https://fontawesome.com/license (Commercial License) Copyright 2023 Fonticons, Inc. --><path d="M215.4 96H144 107.8 96v8.8V144v40.4 89L.2 202.5c1.6-18.1 10.9-34.9 25.7-45.8L48 140.3V96c0-26.5 21.5-48 48-48h76.6l49.9-36.9C232.2 3.9 243.9 0 256 0s23.8 3.9 33.5 11L339.4 48H416c26.5 0 48 21.5 48 48v44.3l22.1 16.4c14.8 10.9 24.1 27.7 25.7 45.8L416 273.4v-89V144 104.8 96H404.2 368 296.6 215.4zM0 448V242.1L217.6 403.3c11.1 8.2 24.6 12.7 38.4 12.7s27.3-4.4 38.4-12.7L512 242.1V448v0c0 35.3-28.7 64-64 64H64c-35.3 0-64-28.7-64-64v0zM176 160H336c8.8 0 16 7.2 16 16s-7.2 16-16 16H176c-8.8 0-16-7.2-16-16s7.2-16 16-16zm0 64H336c8.8 0 16 7.2 16 16s-7.2 16-16 16H176c-8.8 0-16-7.2-16-16s7.2-16 16-16z"/></svg>
                            <%= profile.email %>
                        </li>
                        <li class="list-item">
                          <!-- <i class="fas fa-map-marker-alt mx-4"></i> -->
                          <svg class="icon" xmlns="http://www.w3.org/2000/svg" height="1em" viewBox="0 0 384 512">
                            <path d="M215.7 499.2C267 435 384 279.4 384 192C384 86 298 0 192 0S0 86 0 192c0 87.4 117 243 168.3 307.2c12.3 15.3 35.1 15.3 47.4 0zM192 128a64 64 0 1 1 0 128 64 64 0 1 1 0-128z"/>
                          </svg>
                          <%= profile.address %>
                        </li>
                        <li class="list-item">
                            <!-- <i class="fas fa-solid fa-user mx-4"></i><%= profile.gender %> -->
                            <svg class="icon" xmlns="http://www.w3.org/2000/svg" height="1em" viewBox="0 0 448 512"><!--! Font Awesome Free 6.4.0 by @fontawesome - https://fontawesome.com License - https://fontawesome.com/license (Commercial License) Copyright 2023 Fonticons, Inc. --><path d="M224 256A128 128 0 1 0 224 0a128 128 0 1 0 0 256zm-45.7 48C79.8 304 0 383.8 0 482.3C0 498.7 13.3 512 29.7 512H418.3c16.4 0 29.7-13.3 29.7-29.7C448 383.8 368.2 304 269.7 304H178.3z"/></svg>
                            <%= profile.gender %>
                        </li>
                      </ul>
                  </div>
                  <div>
                      <h5 class="text-uppercase bg-white text-dark py-2 rounded-pill">skills</h5>
                      <ul class="list text-white-50 ml-3 py-2 text-left text-capitalize">
                          <%if !curriculum_vitae.core_skill.blank?%>
                          <h5>Main Skill</h5>
                          <li class="list-item"><%= curriculum_vitae.core_skill&.title %></li>
                            ( <%= curriculum_vitae.core_skill&.desc %> )
                          <%end%>
                          <br>
                          <%if !curriculum_vitae.supportive_skills.blank?%>
                          <h5>More Skills</h5>
                            <% curriculum_vitae.supportive_skills.each do |supportive_skill| %>
                          <li class="list-item">
                            <%= supportive_skill.title %>
                            <br>
                            ( <%= supportive_skill.desc %> )
                            <%# <%= supportive_skill.desc %>
                           
                          </li>
                            <% end %>
                            <% end %>
                            
                        </ul>
                  </div>
             
              </div> 
  
              <!-- <div class="col-lg-8 bg-light text-dark py-4 px-5"> -->
              <div class="col-7 bg-light text-dark py-4 px-5">
                  <div class="header-right">
                      <h4 class="text-uppercase">profile</h4>
                      <hr>
                      <p>
                          <%= curriculum_vitae.profile_desc %>
                      </p>
  
                  </div>
                  <div>
                      <% if !curriculum_vitae.projects.blank? %>
                      <h4 class="text-uppercase">Project</h4>
                      <hr>
                      <ul class="list">
  
                          <% curriculum_vitae.projects.each do |project| %>
                          <li class="list-item">
                              <h5 class="display-6 text-uppercase"><%= project.title %></h5>
                              
  
                              <h6 class="text-uppercase text-black-50">
                                  <%if !project.start_date.blank? %>
                              <span class="jobster-timeline-time"><%=project.start_date%> 
                               <%= project.end_date.blank? ? " inProgerss" : " to "+project.end_date.to_s %>
                               </span>
                              <%end%>
                              </h6>
                              <p><%= project.desc %></p>
  
                          </li>
                          <% end %>
                          <%# <li class="list-item">
                              <h5 class="display-6 text-uppercase">online media marketing</h5>
                              <h6 class="text-uppercase text-black-50">company name /2020.present</h6>
                              <p>Lorem ipsum dolor, sit amet consectetur adipisicing elit. Assumenda modi distinctio voluptatibus doloribus nulla doloremque mollitia rem. Tempora esse perferendis asperiores libero, itaque, mollitia, necessitatibus quaerat ducimus omnis voluptatum autem.
                              </p>
                              
                          </li> %>
                          <% end%>
                      </ul>
                  </div>
               </div>   
          </div>
      </div>
  </div>