  </head>
  <style>
    *{
      margin: 2px;
    }
    .header_gr
    {
      padding: 14px;
    }
   .table_gr
   {
    padding: 0 50px 0 50px;
   }
   .table_gr table
   {
    border: 2px solid black;
   }
   /* .table_gr table tbody tr:first-child.class_gr */
   .table_gr table tbody tr:first-child
   {
    background-color: #43a5b0;
   }
   .bg-secondary {
    /* background-color: #6c757d!important; */
    background: linear-gradient(45deg, #744848f5, #77d8c9bf);
    }
  </style>
  <body>
    <div>
      <% if user_signed_in? %>
    <%= render 'shared/navbar' %>
     <% end %>
    </div>
    <div style="text-align: center">
      <% if notice || alert%>
    <p style="color: green" class="notice"><%= notice %></p>
    <p style="color: red" class="alert"><%= alert %></p>
       <% end%>
    <%= yield %>
  </div>
  </body>
</html>
