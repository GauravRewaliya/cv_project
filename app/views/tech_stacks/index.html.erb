<style>
  .btn-primary{
    text-decoration: none;
    text-color: red;
  }
</style>
<div style="position: relative;">
<h1 class="pos_l">Tech_stack\Skills</h1>

<form action="tech_stacks"  class="form-inline pos_r">
  <input type="text" name="search" placeholder="Search" class="form-control mr-sm-2 form-control-lg">
  <button type="submit" class="btn btn-lg btn-success">Search</button>
</form>
</div>


<div class="table_gr" >
  <table border=1 class="table table-hover table-striped " , style="border: 2px solid black;">
    <tr style="color:White">
      <th>Title</th>
      <th>Description</th>
      <th>Edit</th>
      <th>Delete</th>
      <th>Show</th>
    </tr>
  <% @tech_stacks.each do |tech_stack| %>
  <tr>
    <td ><h6><%= tech_stack.title %></h6></td>
    <td><p><%= tech_stack.desc %></p></td>
    <td><%= link_to "Edit", edit_tech_stack_path(tech_stack),class: "material-symbols-outlined" %></td>
    <td><%= button_to "Delete" , tech_stack , method: :delete ,class: "btn-primary"%></td>
    <td>
    <p>
      <%= link_to "Show this tech_stack", tech_stack %>
    </p>
  </td>
  </tr>
  <% end %>
  </table>
</div>

<div style="position: relative;">
<%= link_to "New tech_stack", new_tech_stack_path ,class: 'btn btn-primary pos_r' %>
<!-- <%= link_to "Home" ,  home_index_path ,class: 'btn btn-primary pos_l'%> -->
</div>